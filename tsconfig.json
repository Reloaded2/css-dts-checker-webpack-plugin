{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ES6"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "declaration": true,
    "baseUrl": "./src/",
    "outDir": "./dist",
  },
  "exclude": ["node_modules", "dist", "examples"]
}
